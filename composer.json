{
    "name": "imedge/ip-list-generator",
    "description": "IP list generator library",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Gelf",
            "email": "thomas@gelf.net"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "IMEdge\\IpListGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IMEdge\\Tests\\IpListGenerator\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.4",
        "imedge/config": "^0.2"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10 | ^2",
        "phpunit/phpunit": "^10.3 || ^9.5",
        "squizlabs/php_codesniffer": "^3.7",
        "symplify/easy-coding-standard": "^12.0.8"
    }
}
